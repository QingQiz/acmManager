@model acmManager.Contest.Dto.GetContestOutput

<div class="card mt-3">
    <div class="card-header noSelect">
        <h5 class="mb-0">
            <button class="btn collapsed" data-target="#contest-result" data-toggle="collapse">
                比赛结果
            </button>
        </h5>
    </div>
    <div class="collapse show" id="contest-result">
        <div class="card-body">
            <div id="contest-result-content">
                <textarea hidden>@Model.Result.Content</textarea>
            </div>
        </div>
    </div>
</div>

